# Zeppelin Configuration
# Copy this to .env and fill in your values

# Storage backend (s3 | gcs | azure | local)
STORAGE_BACKEND=s3

# AWS S3 / MinIO / R2
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
S3_BUCKET=zeppelin
S3_ENDPOINT=
S3_ALLOW_HTTP=false

# GCS
GCS_SERVICE_ACCOUNT_PATH=

# Azure
AZURE_ACCOUNT=
AZURE_ACCESS_KEY=

# Test configuration
TEST_BACKEND=s3
TEST_S3_BUCKET=zeppelin-test

# Server
ZEPPELIN_HOST=0.0.0.0
ZEPPELIN_PORT=8080

# Logging
RUST_LOG=info
